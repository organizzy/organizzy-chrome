{
  "manifest_version": 2,

  "name": "Organizzy",
  "description": "Organize - Easily",
  "version": "0.5.2",
  
  "author": "Abi Hafshin",
  
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon.png"
  },
  
  "offline_enabled": false,
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://organizzy.org; object-src 'self'",
  "permissions": [
    "https://organizzy.org/"
  ],
  "homepage_url": "https://organizzy.org/",
  
  "browser_action": {
    "default_icon": "icon-32.png",
    "default_popup": "popup.html"
  }
}
